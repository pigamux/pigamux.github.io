        <tuv xml:lang="en-US">
            <seg>Your neurons create brain waves.&#10;</seg>
        </tuv>
        <tuv xml:lang="pt-PT">
            <seg>Os teus neurónios criam ondas cerebrais.&#10;</seg>
        </tuv>
    </tu>

    <tu tuid="1914150024" creationdate="20210520T173915Z" datatype="plaintext" srclang="en-US">
        <prop type="x-MateCAT-id_job">4016516</prop>
